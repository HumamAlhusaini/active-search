// feedback
// https://daisyui.com/components/toast
package components

import "github.com/HumamAlhusaini/active-search/internal/model"

templ Toast(toast model.Toast) {
	<div name={ toast.Name } class={ "toast", toast.ToastClass }>
		<div class={ "alert", toast.AlertClass }>
			{ children... }
		</div>
	</div>
}
